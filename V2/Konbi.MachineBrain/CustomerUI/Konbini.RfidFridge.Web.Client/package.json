    "lodash": "^4.17.15",
    "materialize-css": "^1.0.0-rc.2",
    "popper.js": "^1.15.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "electron": "^3.0.11",
